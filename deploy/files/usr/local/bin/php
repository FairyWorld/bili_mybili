#!/bin/bash
if [[ "$1" == "-v" ]]; then
    echo "PHP 8.3.9"
    exit 0
fi
if [[ "$1" == "-l" ]]; then
    exit 0
fi

args=("$@")
index=0
for i in "$@"
do
    if [ "$i" == "-d" ]; then
        unset 'args[$index]'
        unset 'args[$index+1]'
    fi
    index=$((index+1))
done


exec frankenphp php-cli ${args[@]}